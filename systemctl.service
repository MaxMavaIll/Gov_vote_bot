[Unit]
Description=Vote
After=network.target

[Service]
Type=simple
User=root
WorkingDirectory=/root/.scripte/Gov_vote_bot/
ExecStart=/root/.scripte/Gov_vote_bot/venv/bin/python /root/.scripte/Gov_vote_bot/bot.py
Restart=on-failure
RestartSec=10
LimitNOFILE=65535

[Install]
WantedBy=multi-user.target